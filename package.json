{
  "name": "proxtop",
  "version": "0.4.3",
  "description": "A desktop application for proxer.me",
  "main": "main.js",
  "scripts": {
    "test": "electron-mocha --opts mocha.opts",
    "start": "./run.sh",
    "dist": "make package",
    "debug": "electron --debug ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kumpelblase2/proxtop.git"
  },
  "keywords": [
    "electron",
    "desktop",
    "anime",
    "proxer"
  ],
  "author": "kumpelblase2",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kumpelblase2/proxtop/issues"
  },
  "homepage": "https://github.com/kumpelblase2/proxtop#readme",
  "devDependencies": {
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "devtron": "1.3.0",
    "electron": "1.4.3",
    "electron-builder": "7.11.2",
    "electron-mocha": "3.1.1",
    "electron-packager": "8.1.0"
  },
  "dependencies": {
    "about-window": "1.4.0",
    "bluebird": "3.4.6",
    "cheerio": "0.22.0",
    "electron-window-state": "3.0.4",
    "lodash": "4.16.4",
    "lowdb": "0.13.1",
    "moment": "2.15.1",
    "opener": "1.4.2",
    "request": "2.75.0",
    "request-promise": "4.1.1",
    "semver": "5.3.0",
    "tough-cookie": "2.3.1",
    "winston": "2.2.0"
  },
  "build": {
    "appId": "de.eternalwings.proxtop",
    "app-category-type": "public.app-category.entertainment",
    "mac": {
      "target": [
        "zip",
        "tar.gz",
        "dmg"
      ]
    },
    "win": {
      "target": [
        "squirrel",
        "zip"
      ],
      "msi": true
    }
  },
  "directories": {
    "app": "_dist",
    "output": "_packaged"
  }
}
