{
  "name": "proxtop",
  "version": "0.3.1",
  "description": "A desktop application for proxer.me",
  "main": "main.js",
  "scripts": {
    "test": "mocha --opts mocha.opts",
    "start": "./run.sh",
    "setup": "./setup.sh",
    "prepare": "mkdir dist && cp -R src package.json bower.json bower_components main.js dist && cd dist && npm install --production",
    "clean": "rm -r dist/ && rm -r packaged/",
    "build:linux": "electron-packager dist \"Proxtop\" --platform=linux --arch=x64 --asar",
    "build:osx": "electron-packager dist \"Proxtop\" --platform=darwin --arch=x64 --asar --icon=src/assets/proxtop_logo_256.icns",
    "build:win": "electron-packager dist \"Proxtop\" --platform=win32 --arch=x64 --asar --icon=src/assets/proxtop_logo_256.ico",
    "build:all": "electron-packager dist --platform=all --arch=x64 --out=packaged/ --overwrite --asar --icon=src/assets/proxtop_logo_256",
    "build": "npm run prepare && npm run build:all"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kumpelblase2/proxtop.git"
  },
  "keywords": [
    "electron",
    "desktop",
    "anime",
    "proxer"
  ],
  "author": "kumpelblase2",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kumpelblase2/proxtop/issues"
  },
  "homepage": "https://github.com/kumpelblase2/proxtop#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "electron-packager": "^6.0.0",
    "electron-prebuilt": "0.37.2",
    "mocha": "^2.4.5"
  },
  "dependencies": {
    "about-window": "^1.3.1",
    "bluebird": "^3.3.4",
    "cheerio": "^0.20.0",
    "electron-window-state": "^2.0.0",
    "lodash": "^4.6.1",
    "lowdb": "^0.12.5",
    "moment": "^2.12.0",
    "opener": "^1.4.1",
    "request-promise": "^2.0.1",
    "semver": "^5.1.0",
    "tough-cookie": "^2.2.2",
    "tough-cookie-filestore": "0.0.1",
    "winston": "^2.2.0"
  }
}
