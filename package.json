{
  "name": "proxtop",
  "version": "0.4.1",
  "description": "A desktop application for proxer.me",
  "main": "main.js",
  "scripts": {
    "test": "electron-mocha --opts mocha.opts",
    "start": "./run.sh",
    "dist": "make package"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kumpelblase2/proxtop.git"
  },
  "keywords": [
    "electron",
    "desktop",
    "anime",
    "proxer"
  ],
  "author": "kumpelblase2",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kumpelblase2/proxtop/issues"
  },
  "homepage": "https://github.com/kumpelblase2/proxtop#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "devtron": "^1.2.1",
    "electron-builder": "^5.11.2",
    "electron-mocha": "^2.3.0",
    "electron-packager": "^7.2.0",
    "electron-prebuilt": "1.2.6"
  },
  "dependencies": {
    "about-window": "^1.3.1",
    "bluebird": "^3.4.1",
    "cheerio": "^0.20.0",
    "electron-window-state": "^3.0.3",
    "lodash": "^4.13.1",
    "lowdb": "^0.13.1",
    "moment": "^2.14.1",
    "opener": "^1.4.1",
    "request-promise": "^3.0.0",
    "semver": "^5.2.0",
    "tough-cookie": "^2.2.2",
    "tough-cookie-filestore": "0.0.1",
    "winston": "^2.2.0"
  },
  "build": {
    "appId": "de.eternalwings.proxtop",
    "app-category-type": "public.app-category.entertainment",
    "mac": {
      "target": [
        "zip",
        "tar.gz"
      ]
    },
    "win": {
      "target": [
        "squirrel",
        "zip"
      ],
      "msi": true
    },
    "linux": {
      "target": [
        "deb",
        "tar.gz",
        "pacman"
      ]
    }
  },
  "directories": {
    "app": "_dist",
    "output": "_packaged"
  }
}
